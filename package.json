{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"next,prisma,email\" -c \"black,magenta,cyan\" \"pnpm dev:next\" \"pnpm dev:prisma\" \"pnpm dev:email\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test --ui",
    "dev:next": "next dev --turbopack --port 3000",
    "dev:prisma": "dotenv -e .env.development -- prisma studio --port 5555 --browser none",
    "dev:email": "email dev --port 3001 --dir ./lib/mails",
    "generate": "prisma generate",
    "migrate:dev": "dotenv -e .env.development -- pnpx prisma migrate dev",
    "migrate:prod": "dotenv -e .env.production -- pnpx prisma migrate deploy",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "i18n:check": "i18n-check --source en --locales messages -f i18next"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
